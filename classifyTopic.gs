/**
 * TorahAnytime Topic Classifier API
 * POST JSON: { "title": "Why Did Hashem Choose Noach?" }
 * Returns: { "Category": "Parsha", "CategoryId": 23, "Subcategory": "Noach", "SubcategoryId": 46 }
 */

function doPost(e) {
  try {
    const body = JSON.parse(e.postData.contents);
    const title = body.title;

    if (!title) throw new Error("Missing title");

    // ðŸ”¹ Step 1: Get embedding vector from Hugging Face
    const inputEmbedding = getEmbeddingFromHF(title);

    // ðŸ”¹ Step 2: Find closest topic/subtopic
    const best = findClosestTopic(inputEmbedding);

    console.log("best: " + JSON.stringify(best));

    let topic = best.topic;
    if (best.similarity < 0.85) {
      topic = "Jewish Understanding";
    }

    console.log("Response: " + JSON.stringify(lookupTopic(topic)));

    return ContentService.createTextOutput(
      JSON.stringify(lookupTopic(topic))
    ).setMimeType(ContentService.MimeType.JSON);
  } catch (err) {
    return ContentService.createTextOutput(
      JSON.stringify({ error: err.message })
    ).setMimeType(ContentService.MimeType.JSON);
  }
}

/**
 * Calls Hugging Face Inference API (intfloat/multilingual-e5-large)
 * and returns the embedding vector for a given title.
 */
function getEmbeddingFromHF(text) {
  console.log("text: " + text);
  const apiUrl =
    "https://router.huggingface.co/hf-inference/models/intfloat/multilingual-e5-large";
  const apiKey = ""; // ðŸ”‘ Replace with your Hugging Face token

  const response = UrlFetchApp.fetch(apiUrl, {
    method: "post",
    headers: {
      Authorization: "Bearer " + apiKey,
      "Content-Type": "application/json",
    },
    payload: JSON.stringify({ inputs: text }),
    muteHttpExceptions: true,
  });

  console.log(response.getResponseCode());
  console.log(response.getContentText());

  if (response.getResponseCode() !== 200)
    throw new Error("Hugging Face API error: " + response.getContentText());

  const result = JSON.parse(response.getContentText());
  // Flatten nested array
  return Array.isArray(result[0]) ? result[0] : result;
}

/**
 * Loads all precomputed embeddings (from Drive) and finds the closest one.
 */
function findClosestTopic(inputEmbedding) {
  const fileId = ""; // ðŸ”¹ replace with your embeddings.jsonl file ID
  const content = DriveApp.getFileById(fileId).getBlob().getDataAsString();
  const lines = content.trim().split("\n");

  let best = { topic: null, similarity: -1 };

  for (const line of lines) {
    const record = JSON.parse(line);
    const sim = cosineSimilarity(inputEmbedding, record.embedding);
    if (sim > best.similarity) best = { topic: record.topic, similarity: sim };
  }

  return best;
}

/**
 * Compute cosine similarity between two vectors.
 */
function cosineSimilarity(a, b) {
  let dot = 0.0,
    normA = 0.0,
    normB = 0.0;
  for (let i = 0; i < a.length; i++) {
    dot += a[i] * b[i];
    normA += a[i] ** 2;
    normB += b[i] ** 2;
  }
  return dot / (Math.sqrt(normA) * Math.sqrt(normB));
}

