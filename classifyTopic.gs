function lookupTopic(pathStr) {
  if (!pathStr || typeof pathStr !== "string") return null;

  if (!pathStr.includes("|")) {
    return topics.find((entry) => entry.categoryName === pathStr);
  }

  const parts = pathStr.split("|");
  const categoryName = parts[0];
  const subCategoryName = parts[1];

  return topics.find(
    (entry) =>
      entry.categoryName === categoryName &&
      entry.subCategoryName === subCategoryName
  );
}
